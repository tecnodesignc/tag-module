{
    "name": "tecnodesignc/tag-module",
    "type": "encore-module",
    "description": "Tag management in EncoreCMS",
    "keywords": [
        "encorecms",
        "tags",
        "taggable"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/encorecms/issues",
        "source": "https://github.com/tecnodesignc/core-module"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "3.8.*",
        "tecnodesignc/page-module": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Tag\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\Tag\\": ".",
            "Modules\\": "modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
